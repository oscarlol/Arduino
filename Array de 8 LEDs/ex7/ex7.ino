//************************** INCLUDE *********************************



// ************************ VARIABLES ********************************
const byte led0 = 5;
boolean A = 0;

//**************************** SETUP *********************************

void setup(){
  Serial.begin(9600);
  pinMode(led0, OUTPUT);
  
}

//*************************** LOOP ***********************************

void loop() {
   
  for(int cnt=0; cnt<5; cnt++)
{
  digitalWrite(led0, HIGH);
  delay(500);
  digitalWrite(led0, LOW);
  delay(500);
  Serial.println(A);
  }
delay(1000);
}
